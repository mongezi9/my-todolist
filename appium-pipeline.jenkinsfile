
node('docker') {
    stage('Initialize'){
        def dockerHome = tool 'myDocker'
        def mavenHome  = tool 'myMaven'
        env.PATH = "${dockerHome}/bin:${mavenHome}/bin:${env.PATH}"
    }
    stage 'Checkout'
        checkout scm
    stage 'Build & UnitTest'
         sh "docker build -t my_to_do_list ."
         sh "docker run -p 8081:8081 -d my_to_do_list"
   
}