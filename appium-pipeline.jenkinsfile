node {
   
    stage('Environment/Bundles Setup') {
        sh "ls"
        sh "docker build -t my_to_do_list ."
     
    }
    stage('Clean') {
        sh 'fastlane clean_xcode'
    }
    stage('Code Sign') {
        sh 'fastlane codesign method:"adhoc"'
    }
    stage('Create Build') {   
        sh 'fastlane create_build'
    }
}
