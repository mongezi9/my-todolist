node {
   
    stage('Environment/Bundles Setup') {
        sh "docker run -v $(pwd):/workspace -v /var/run/docker.sock:/var/run/docker.sock -v $(which docker):$(which docker) marcellodesales/jenkinsfile-runner"
        sh "docker build -t my_to_do_list ."
     
    }
    stage('Clean') {
        sh 'fastlane clean_xcode'
    }
    stage('Code Sign') {
        sh 'fastlane codesign method:"adhoc"'
    }
    stage('Create Build') {   
        sh 'fastlane create_build'
    }
}
